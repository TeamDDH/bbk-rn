{
  "parser": "babel-eslint",
  "env": {
    "browser": false
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "global-require": 0,
    "no-use-before-define": 0,
    "max-len": 0,
    "no-console": 0,
    "no-undef": 2,
    "no-unused-vars": 1,
    "block-scoped-var": 0,
    "complexity": 0,
    "consistent-return": 0,
    "generator-star-spacing": 0,
    "no-return-assign": 1,
    "react/self-closing-comp": 1,
    "react/prop-types": 0,
    "react/display-name": 0,
    "react/no-string-refs": 0,
    "space-before-function-paren": 0
  },
  "globals": {
    "window": true,
    "fetch": true,
    "console": true,
    "Promise": true,
    "__DEV__": true,
    "__APP__": true,
    "__ANDROID__": true,
    "__IOS__": true
  }
}
